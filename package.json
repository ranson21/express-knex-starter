{
  "name": "express-knex-starter",
  "version": "1.0.0",
  "description": "Express Knex Starter",
  "main": "src/index.js",
  "scripts": {
    "pretest": "eslint .",
    "test": "jest",
    "build": "backpack build",
    "start": "backpack",
    "migrate": "knex migrate:latest"
  },
  "engines": {
    "node": ">=14.17.0 <15"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.6.0",
    "@babel/node": "^7.6.1",
    "babel-jest": "^24.9.0",
    "backpack-core": "^0.8.4",
    "dotenv": "^8.1.0",
    "knex": "^0.95.14",
    "pg": "^8.7.1"
  },
  "devDependencies": {
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-regenerator": "^7.4.5",
    "@babel/plugin-transform-runtime": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "eslint": "^6.4.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-config-xo": "^0.26.0",
    "eslint-plugin-prettier": "^3.1.0",
    "husky": "^3.0.5",
    "jest": "^25.0.0",
    "lint-staged": "^9.2.5",
    "prettier": "^1.18.2",
    "webpack-node-externals": "^3.0.0"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": "error"
    },
    "plugins": [
      "prettier"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "^api": "<rootDir>/src/index.js",
      "^middleware/(.*)$": "<rootDir>/src/middleware/$1",
      "^utils/(.*)$": "<rootDir>/src/utils/$1",
      "^src/(.*)$": "<rootDir>/src/$1",
      "^test/(.*)$": "<rootDir>/test/$1"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/*/index.js"
    ]
  }
}
